{
    "$schema": "http://json-schema.org/schema#",
    "$id": "https://raw.githubusercontent.com/Levertion/minecraft-json-schemas/master/java/data/loot_table.json",
    "type": "object",
    "title": "根标签。",
    "additionalProperties": false,
    "properties": {
        "additionalProperties": false,
        "type": {
            "type": "string",
            "title": "Loot table type",
            "description": "该战利品表的类型。将被游戏用于验证函数和条件的使用。",
            "enum": [
                "minecraft:advancement_reward",
                "minecraft:block",
                "minecraft:chest",
                "minecraft:empty",
                "minecraft:entity",
                "minecraft:fishing"
            ]
        },
        "functions": {
            "type": "array",
            "title": "战利品表函数",
            "items": {
                "$ref": "#/definitions/function"
            }
        },
        "pools": {
            "type": "array",
            "title": "Loot pools",
            "description": "所有可用的随机池。",
            "items": {
                "additionalProperties": false,
                "properties": {
                    "functions": {
                        "type": "array",
                        "title": "随机池函数。",
                        "items": {
                            "$ref": "#/definitions/function"
                        }
                    },
                    "conditions": {
                        "$ref": "#/definitions/conditions",
                        "title": "随机池条件、",
                        "description": "The conditions which must pass in order for this pool to be used"
                    },
                    "rolls": {
                        "$ref": "#/definitions/random",
                        "title": "抽取次数。",
                        "description": "在该随机池中抽取的次数。"
                    },
                    "bonus_rolls": {
                        "$ref": "#/definitions/random",
                        "title": "奖励抽取次数。",
                        "description": "每一点幸运效果所增加的抽取次数。"
                    },
                    "entries": {
                        "type": "array",
                        "title": "项目列表。",
                        "description": "每一次抽取都会从该列表中随机选择。选中某个项目的几率由它的 'weight' 权重决定。",
                        "items": {
                            "$ref": "#/definitions/entry"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "conditions": {
            "type": "array",
            "title": "条件",
            "description": "该随机池/项目被使用时应达成的条件。",
            "items": {
                "$ref": "#/definitions/condition"
            }
        },
        "condition": {
            "oneOf": [
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:entity_properties"
                            ],
                            "title": "测试实体属性。",
                            "descriptions": "测试 entity 中指定的实体能否通过 predicate 中的断言。"
                        },
                        "entity": {
                            "$ref": "#/definitions/entity"
                        },
                        "predicate": {
                            "$ref": "../shared/predicate.json#/definitions/entity"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:entity_scores"
                            ]
                        },
                        "entity": {
                            "$ref": "#/definitions/entity"
                        },
                        "scores": {
                            "type": "object",
                            "title": "检查的分数。",
                            "description": "要检测的分数。",
                            "additionalProperties": {
                                "$ref": "../shared/range.json"
                            }
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:random_chance"
                            ],
                            "title": "随机几率。",
                            "description": "游戏会生成一个 0.0 到 1.0 的随机数，当该数字小于等于被比数时，条件通过。"
                        },
                        "chance": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 1,
                            "title": "被比数。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "type": "string",
                            "enum": [
                                "minecraft:random_chance_with_looting"
                            ],
                            "title": "受抢夺魔咒影响的随机几率。",
                            "description": "游戏会生成一个 0.0 到 1.0 的随机数，当该数字小于等于[b]被比数[/b]时，条件通过。击杀者的抢夺附魔等级会影响被比数的大小。"
                        },
                        "chance": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 1,
                            "title": "被比数的基础值。"
                        },
                        "looting_multiplier": {
                            "type": "number",
                            "title": "每一级抢夺附魔所增加的被比数。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:killed_by_player"
                            ],
                            "description": "当前实体被玩家击杀时，条件通过。"
                        },
                        "inverse": {
                            "type": "boolean",
                            "title": "Inverse",
                            "description": "如果设置为 true，则变为击杀者不是玩家时条件通过。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:block_state_property"
                            ],
                            "description": "匹配被破坏方块。"
                        },
                        "block": {
                            "$ref": "../shared/block_name.json",
                            "title": "方块 ID。"
                        },
                        "properties": {
                            "type": "object",
                            "title": "方块状态。",
                            "description": "一系列方块状态的键-值对。",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:damage_source_properties"
                            ],
                            "title": "匹配该实体死亡时受到的伤害。"
                        },
                        "predicate": {
                            "$ref": "../shared/predicate.json#/definitions/damage"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:location_check"
                            ],
                            "description": "当前位置满足指定断言时，条件通过。"
                        },
                        "predicate": {
                            "$ref": "../shared/predicate.json#/definitions/location"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:match_tool"
                            ],
                            "description": "匹配玩家使用的工具。"
                        },
                        "predicate": {
                            "$ref": "../shared/predicate.json#/definitions/item"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:survives_explosion"
                            ],
                            "description": "如果该方块是被爆炸破坏的，该条件有 1/爆炸半径 的几率通过；如果该方块是被玩家正常挖掘的，该条件永远通过。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:table_bonus"
                            ],
                            "description": "分别定义不同级别的附魔等级下条件的通过几率。"
                        },
                        "enchantment": {
                            "$ref": "../shared/enchantment_name.json",
                            "title": "魔咒。",
                            "description": "探测的魔咒 ID。"
                        },
                        "chances": {
                            "type": "array",
                            "title": "几率。",
                            "description": "每个魔咒等级下的通过几率。",
                            "items": {
                                "type": "number"
                            }
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:weather_check"
                            ],
                            "description": "检测当时的天气。"
                        },
                        "raining": {
                            "type": "boolean",
                            "title": "是否下雨。"
                        },
                        "thundering": {
                            "type": "boolean",
                            "title": "是否打雷。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:alternative"
                            ],
                            "description": "析取范式。即，若指定的条件中有一个通过，则该条件通过；否则，该条件不通过。"
                        },
                        "terms": {
                            "$ref": "#/definitions/conditions",
                            "title": "条件列表。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "condition": {
                            "enum": [
                                "minecraft:inverted"
                            ],
                            "description": "取反。即，若指定的条件通过，则该条件不通过；否则，该条件通过。"
                        },
                        "term": {
                            "$ref": "#/definitions/condition",
                            "title": "指定的条件。"
                        }
                    }
                }
            ],
            "required": [
                "condition"
            ]
        },
        "entity": {
            "title": "要测试的实体。",
            "description": "this 指代当前死亡的实体，killer 指代击杀者，killer_player 指代击杀玩家（如果存在的话）。",
            "enum": [
                "this",
                "killer",
                "killer_player"
            ]
        },
        "function": {
            "title": "Function",
            "description": "一个改变物品属性的函数。",
            "required": [
                "function"
            ],
            "oneOf": [
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:set_count"
                            ],
                            "title": "设置物品堆叠数量。"
                        },
                        "count": {
                            "$ref": "#/definitions/random",
                            "title": "堆叠数量。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:set_damage"
                            ],
                            "title": "设置物品耐久度。"
                        },
                        "damage": {
                            "$ref": "#/definitions/random",
                            "description": "随机设置的剩余耐久度。0.35 表示剩余 35% 耐久度。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:set_nbt"
                            ],
                            "title": "给该物品设置 NBT。"
                        },
                        "tag": {
                            "type": "string",
                            "title": "NBT string",
                            "description": "要添加的 NBT。注意转义。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:set_attributes"
                            ]
                        },
                        "modifiers": {
                            "type": "array",
                            "title": "向该物品添加属性修饰器。",
                            "items": {
                                "additionalProperties": false,
                                "title": "A single modifier",
                                "required": [
                                    "attribute",
                                    "name",
                                    "amount",
                                    "operation",
                                    "slot"
                                ],
                                "properties": {
                                    "attribute": {
                                        "enum": [
                                            "generic.maxHealth",
                                            "generic.followRange",
                                            "generic.knockbackResistance",
                                            "generic.movementSpeed",
                                            "generic.attackDamage",
                                            "generic.armor",
                                            "generic.armorToughness"
                                        ],
                                        "title": "应用的属性。"
                                    },
                                    "name": {
                                        "title": "自定义名称。",
                                        "description": "属性的名字。可以是任意字符串。",
                                        "type": "string"
                                    },
                                    "amount": {
                                        "$ref": "#/definitions/random",
                                        "title": "编辑量。",
                                        "description": "对该属性的编辑数量。"
                                    },
                                    "operation": {
                                        "enum": [
                                            "addition",
                                            "multiply_base",
                                            "multiply_total"
                                        ],
                                        "title": "应用的属性"
                                    },
                                    "slot": {
                                        "title": "Applied slot",
                                        "description": "应用该属性的物品栏位。",
                                        "oneOf": [
                                            {
                                                "type": "array",
                                                "items": {
                                                    "enum": [
                                                        "mainhand",
                                                        "offhand",
                                                        "feet",
                                                        "legs",
                                                        "chest",
                                                        "head"
                                                    ]
                                                }
                                            },
                                            {
                                                "enum": [
                                                    "mainhand",
                                                    "offhand",
                                                    "feet",
                                                    "legs",
                                                    "chest",
                                                    "head"
                                                ]
                                            }
                                        ]
                                    },
                                    "id": {
                                        "title": "字符串形式的 UUID（可选）",
                                        "description": "一个给予该属性的 UUID。如果留空将会由游戏随机生成。通过指定两个 UUID 相同的属性修饰器，可以让物品间互相覆盖属性修饰效果（可能导致 bug）。",
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:enchant_randomly"
                            ],
                            "title": "选择随机魔咒。",
                            "description": "从魔咒列表中随机选择魔咒。"
                        },
                        "enchantments": {
                            "type": "array",
                            "items": {
                                "$ref": "../shared/enchantment_name.json"
                            },
                            "title": "魔咒列表。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:enchant_with_levels",
                                "enchant_with_levels"
                            ],
                            "title": "附魔台。",
                            "description": "像该物品被附魔台附魔一样进行随机附魔。"
                        },
                        "levels": {
                            "$ref": "#/definitions/random"
                        },
                        "treasure": {
                            "type": "boolean",
                            "title": "是否使用宝藏附魔。",
                            "description": "附魔结果能否包含宝藏魔咒。"
                        }
                    },
                    "required": [
                        "levels"
                    ]
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:looting_enchant"
                            ],
                            "title": "由击杀者的抢夺魔咒等级调整物品的数量。"
                        },
                        "count": {
                            "$ref": "#/definitions/random",
                            "description": "每一级抢夺魔咒增加的物品数量。"
                        },
                        "limit": {
                            "type": "number",
                            "title": "Maximum",
                            "description": "物品数量的最大限制。0 代表无限制。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:furnace_smelt"
                            ],
                            "title": "熔炼。",
                            "description": "如果可以的话，对该物品进行熔炼。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:exploration_map"
                            ],
                            "description": "将普通的地图物品变为一个指引到某个结构的探险地图。"
                        },
                        "destination": {
                            "enum": [
                                "Buried_Treasure",
                                "Desert_Pyramid",
                                "EndCity",
                                "Fortress",
                                "Igloo",
                                "Jungle_Pyramid",
                                "Mansion",
                                "Mineshaft",
                                "Monument",
                                "Ocean_Ruin",
                                "Shipwreck",
                                "Stronghold",
                                "Swamp_Hut",
                                "Village"
                            ],
                            "description": "目标结构的 ID。"
                        },
                        "decoration": {
                            "enum": [
                                "player",
                                "frame",
                                "red_marker",
                                "blue_marker",
                                "target_x",
                                "target_point",
                                "player_off_map",
                                "player_off_limits",
                                "mansion",
                                "monument",
                                "banner_white",
                                "banner_orange",
                                "banner_magenta",
                                "banner_light_blue",
                                "banner_yellow",
                                "banner_lime",
                                "banner_pink",
                                "banner_gray",
                                "banner_light_gray",
                                "banner_cyan",
                                "banner_purple",
                                "banner_blue",
                                "banner_brown",
                                "banner_green",
                                "banner_red",
                                "banner_black",
                                "red_x"
                            ],
                            "description": "该结构在地图中显示的图标。如果设置为 mansion 或 monument，地图物品的图标将会改变。"
                        },
                        "zoom": {
                            "type": "integer",
                            "description": "地图缩放等级。默认为 2。"
                        },
                        "search_radius": {
                            "type": "integer",
                            "description": "搜寻的区块半径。搜寻的区域是方形，不是圆形。设置为 0 将只在当前区块搜寻，设置为 1 将会搜寻当前区块和周围的 8 个区块，以此类推。默认为 50。"
                        },
                        "skip_existing_chunks": {
                            "type": "boolean",
                            "description": "是否不在已经生成过的区块搜寻。默认为 true。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:set_name"
                            ],
                            "title": "设置该物品的显示名。"
                        },
                        "name": {
                            "$ref": "../shared/text_component.json",
                            "description": "显示名。"
                        },
                        "entity": {
                            "$ref": "#/definitions/entity",
                            "description": "如果存在，将被用于解析 name 中的 score 和 entity。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:set_lore"
                            ],
                            "title": "设置该物品的下标。"
                        },
                        "lore": {
                            "type": "array",
                            "items": {
                                "$ref": "../shared/text_component.json",
                                "description": "一行下标。"
                            }
                        },
                        "entity": {
                            "$ref": "#/definitions/entity",
                            "description": "如果存在，将被用于解析 JSON 聊天文本中的 entity 和 score。"
                        },
                        "replace": {
                            "type": "boolean",
                            "description": "是否替代原有的下标。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:fill_player_head"
                            ],
                            "title": "将玩家头颅的皮肤信息设置为指定玩家。"
                        },
                        "entity": {
                            "$ref": "#/definitions/entity",
                            "description": "如果指定的实体不是玩家，或者该物品不是玩家头颅，什么都不会发生。"
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "oneOf": [
                        {
                            "properties": {
                                "formula": {
                                    "enum": [
                                        "binomial_with_bonus_count"
                                    ],
                                    "description": "将按照二项分布生成随机数。"
                                },
                                "parameters": {
                                    "additionalProperties": false,
                                    "properties": {
                                        "extraRounds": {
                                            "type": "integer"
                                        },
                                        "probability": {
                                            "type": "number"
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "formula": {
                                    "enum": [
                                        "uniform_bonus_count"
                                    ],
                                    "description": "将按照均匀分布生成随机数。"
                                },
                                "parameters": {
                                    "additionalProperties": false,
                                    "properties": {
                                        "bonusMultiplier": {
                                            "type": "number"
                                        }
                                    }
                                }
                            }
                        },
                        {
                            "properties": {
                                "formula": {
                                    "enum": [
                                        "ore_drops"
                                    ],
                                    "description": "应用公式 count * (max(0, random(0..1) - 1) + 1)。"
                                },
                                "parameters": {
                                    "additionalProperties": false
                                }
                            }
                        }
                    ],
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:apply_bonus"
                            ],
                            "title": "应用奖励。",
                            "description": "应用预设的奖励公式之一。"
                        },
                        "enchantment": {
                            "$ref": "../shared/enchantment_name.json",
                            "description": "用于计算的魔咒 ID。"
                        },
                        "formula": {
                            "description": "使用的奖励公式。"
                        },
                        "parameters": {}
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:copy_name"
                            ],
                            "title": "复制名称。",
                            "description": "从方块实体复制显示名称到物品。"
                        },
                        "source": {
                            "enum": [
                                "block_entity"
                            ]
                        }
                    }
                },
                {
                    "additionalProperties": false,
                    "properties": {
                        "function": {
                            "enum": [
                                "minecraft:copy_nbt"
                            ],
                            "title": "复制 NBT。",
                            "description": "将 NBT 从源复制到目标。"
                        },
                        "source": {
                            "$ref": "#/definitions/entity",
                            "enum": [
                                "block_entity"
                            ]
                        },
                        "ops": {
                            "type": "array",
                            "title": "操作。",
                            "description": "所有复制操作。",
                            "items": {
